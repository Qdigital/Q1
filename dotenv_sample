# sample .env file
# this will be overwritten by
# docker-compose - the actual env
# cloud-gov - bootstrap.sh based on cloud.gov env vars
# bsp - by ansible
DB_NAME=dashboard
DB_USER=root
DB_PASSWORD=mysql
DB_HOST=database
# DB_DEBUG: 
#   TRUE: fail on database errors on migration, display to user
#   FALSE: continue regardless of errors
DB_DEBUG=TRUE

